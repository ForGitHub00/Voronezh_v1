<Window x:Class="Voronezh_v1.SeamTracking"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Voronezh_v1"
        mc:Ignorable="d"
        Title="SeamTracking"
        Height="700"
        Width="700">
    <Grid>
        <StackPanel>
            <Button Name="bt_start"
                    Content="Start"
                    Margin="5"
                    Click="bt_start_Click">
            </Button>
            <TextBox Margin="5"
                     Name="tb_position"></TextBox>
            <TextBox Margin="5"
                     Name="tb_work"></TextBox>
            <TextBox Margin="5"
                     Name="tb_recive_point"></TextBox>
            <TextBox Margin="5"
                     Name="tb_send_point"></TextBox>
            <TextBox Margin="5"
                     Name="tb_cur_point"></TextBox>
            <Button Name="bt_stop"
                    Content="Stop"
                    Margin="5"
                    Click="bt_stop_Click">
            </Button>
        </StackPanel>
    </Grid>
</Window>
